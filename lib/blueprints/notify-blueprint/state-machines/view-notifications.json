{
  "Comment": "View notifications sent via Notify Service",
  "name": "View Notifications",
  "version": "1.0",
  "categories": [
    "notify"
  ],
  "instigators": [
    "user"
  ],
  "instigatorGroup": "app",
  "StartAt": "AwaitingHumanInput",
  "States": {
    "AwaitingHumanInput": {
      "Type": "Task",
      "Resource": "module:awaitingHumanInput",
      "ResourceConfig": {
        "uiName": "notify_viewNotifications",
        "dataPath": "$"
      },
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "notify_admin",
      "allows": [
        "*"
      ]
    }
  ]
}
