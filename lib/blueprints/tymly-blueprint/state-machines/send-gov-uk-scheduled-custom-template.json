{
  "Comment": "Send scheduled custom template",
  "name": "Send scheduled custom template",
  "version": "1.0",
  "categories": [
    "govUkNotify"
  ],
  "StartAt": "Logging",
  "States": {
    "Logging": {
      "Type": "Task",
      "Resource": "module:logging",
      "ResourceConfig": {
        "template": "Sending on schedule!"
      },
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "$everyone",
      "allows": [
        "*"
      ]
    }
  ]
}
