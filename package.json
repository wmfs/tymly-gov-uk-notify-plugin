  "homepage": "https://github.com/wmfs/tymly-gov-uk-notify-plugin#readme",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "plugin",
    "notifications",
    "alerting",
    "alerts",
    "notify",
    "GOV UK"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-gov-uk-notify-plugin.git"
  },
